<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:viewModels="using:GenericCellularAutomation.Presentation.Rules.ViewModels"
             xmlns:views="using:GenericCellularAutomation.Avalonia.Views"
             x:Class="GenericCellularAutomation.Avalonia.Views.RuleSetCollectionView"
             x:DataType="viewModels:RuleSetCollectionViewModel"
             x:Name="Root">
    <DockPanel Classes="stack-items-vertically">
        <ItemsControl ItemsSource="{Binding RuleSetViewModels}">
            <DataTemplate>
                <views:RuleSetView/>
            </DataTemplate>
        </ItemsControl>
    </DockPanel>
</UserControl>