<ResourceDictionary xmlns="https://github.com/avaloniaui"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:dataTypes="using:SimulationStorm.Collections.StorageControl"
                    xmlns:ti="using:TablerIcons"
                    xmlns:me="using:SimulationStorm.Avalonia.MarkupExtensions">
    <DataTemplate DataType="dataTypes:CollectionStorageLocation" x:Key="Templates.CollectionStorageLocationTemplate">
        <StackPanel Classes="has-spacing"
                    Orientation="Horizontal">
            <ti:TablerIcon Icon="{me:DynamicResourceBinding StringFormat='Icons.Collections.StorageLocation.{0}'}"/>
            <TextBlock Text="{me:DynamicResourceBinding StringFormat='Strings.Collections.StorageLocation.{0}'}"/>
        </StackPanel>
    </DataTemplate>
</ResourceDictionary>