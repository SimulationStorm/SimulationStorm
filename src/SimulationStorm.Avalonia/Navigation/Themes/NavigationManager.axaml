<ResourceDictionary xmlns="https://github.com/avaloniaui"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:local="using:SimulationStorm.Avalonia.Navigation">
    <ControlTheme x:Key="{x:Type local:NavigationManager}" TargetType="local:NavigationManager">
        <Setter Property="Template">
            <ControlTemplate>
                <TransitioningContentControl x:Name="PART_ContentControl"/>
            </ControlTemplate>
        </Setter>
    </ControlTheme>
</ResourceDictionary>