<UserControl x:Class="SimulationStorm.Simulation.Statistics.Avalonia.RenderingStats.RenderingStatsBarChartView"
             xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:lvc="using:LiveChartsCore.SkiaSharpView.Avalonia"
             xmlns:charts="using:SimulationStorm.Simulation.Statistics.Presentation.RenderingStats.Charts"
             x:DataType="charts:RenderingBarChartViewModel">
    <lvc:CartesianChart Series="{Binding Series}"
                             XAxes="{Binding XAxes}"
                             YAxes="{Binding YAxes}"/>
</UserControl>